<!DOCTYPE html>
<html lang="en">

<head>

    <!-- Title -->
    <title>Dashboard</title>

    <!-- Meta -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="author" content="DexignZone">
    <meta name="robots" content="index, follow">

    <meta name="keywords"
          content="Ticketing, Admin Dashboard, Tailwind CSS, Ticket Management, Template, e-commerce, online shopping, fashion, UX/UI, mobile development, web development, Bootstrap 5, mobile app template, mobile commerce, Bootstrap Template, Admin Panel, Dashboard UI Kit, HTML5, CSS3, Responsive Web App, Bootstrap 4, User Interface Design, android, mobile, mobile template, mobile app, ui kit, dark layout, app, mobile web, pwa, web app, Bootstrap, mobile UI, app template, responsive design, trendy app, modern UI, technology, User-Friendly Interface, PWA (Progressive Web App), Mobile Ordering, Innovative Technology, App Development, Mobile App Development, Product Showcase, Customizable, Modern Design, UI/UX Design, Mobile App Design, Web Application, Digital Presence">

    <meta name="description"
          content="Tixia is a sophisticated Ticketing Admin Dashboard Tailwind CSS Template designed to streamline your ticket management system. With its modern and intuitive interface, Tixia offers a comprehensive solution for administrators to efficiently handle ticket-related tasks. This Tailwind CSS template provides a seamless user experience, allowing easy navigation and quick access to essential features.">

    <!-- Mobile Specific -->
    <meta name="viewport"
          content="width=device-width, initial-scale=1, maximum-scale=1, minimum-scale=1, minimal-ui, viewport-fit=cover">

    <!-- FAVICONS ICON -->
    <link rel="shortcut icon" type="image/png" href="~/Content/assets/images/favicon.png">

    <!-- ICONS -->
    <link rel="stylesheet" href="~/Content/assets/icons/flaticon/flaticon.css">
    <link rel="stylesheet" href="~/Content/assets/icons/fontawesome/css/all.min.css">
    <link rel="stylesheet" href="~/Content/assets/icons/line-awesome/css/line-awesome.min.css">
    <!-- NICE SELECT -->
    <link href="~/Content/assets/vendor/niceselect/css/nice-select.css" rel="stylesheet">
    <!-- STYLESHEETS -->
    <link href="~/Content/assets/vendor/powerful-calendar/style.css" rel="stylesheet">
    <link href="~/Content/assets/vendor/powerful-calendar/theme.css" rel="stylesheet">

    <!-- STYLE CSS -->
    <link href="~/Content/assets/css/style.css" rel="stylesheet">

    <style>
        .sidebar-right-trigger {
            display: none !important;
        }
    </style>
</head>

<body class="selection:text-white selection:bg-primary">
    <div id="preloader">
        <div class="sk-three-bounce">
            <div class="sk-child sk-bounce1"></div>
            <div class="sk-child sk-bounce2"></div>
            <div class="sk-child sk-bounce3"></div>
        </div>
    </div>

    <div id="main-wrapper">

        <!-- Header start -->
        @Html.Partial("_HeaderDashboard")
        <!-- Header end -->
        <!-- Sidebar start -->
        @Html.Partial("_SidebarDashboard")

        <!-- Content body start -->
        @RenderBody()

        @Html.Partial("_FooterDashboard")
    </div>
    <!-- Required vendors -->
    <script src="~/Content/assets/vendor/global/global.min.js"></script>
    <script src="~/Content/assets/vendor/chart.js/chart.bundle.min.js"></script>
    <script src="~/Content/assets/vendor/apexchart/apexchart.js"></script>
    <script src="~/Content/assets/vendor/peity/jquery.peity.min.js"></script>

    <!-- Dashboard 1 -->
    <script src="~/Content/assets/js/dashboard/dashboard-1.js"></script>

    <script src="~/Content/assets/vendor/niceselect/js/jquery.nice-select.min.js"></script>
    <script src="~/Content/assets/vendor/powerful-calendar/calendar.min.js"></script>
    <script src="~/Content/assets/js/custom.js"></script>
    <script src="~/Content/assets/js/deznav-init.js"></script>
    <script src="~/Content/assets/js/demo.js"></script>
    <script src="~/Content/assets/js/styleSwitcher.js"></script>
</body>
</html>